{
  "name": "test-drive",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/server.js --mode production",
    "dev": "nodemon ./src/server.js -p 8080",
    "devfs": "nodemon ./src/server.js -p 8070 --persistence FS",
    "devmemory": "nodemon ./src/server.js -p 8060 --persistence MEMORY",
    "admin": "node ./src/config/singleton.js",
    "data": "node ./src/dao/data/data.mocks.js",
    "simple": "artillery quick --count 10 --num 50 http://localhost:8080/api/simple -o ./src/tests/performance/simple.json",
    "complex": "artillery quick --count 10 --num 50 http://localhost:8080/api/complex -o ./src/tests/performance/complex.json",
    "test-advance": "artillery run ./config.yaml --output ./src/tests/performance/create_destroy.json",
    "test-report": "artillery report ./src/tests/performance/create_destroy.json",
    "image": "docker build -t fmd --platform linux/amd64 .",
    "container": "docker run -p 8000:8000 fmd ",
    "image-dev": "docker build -t fmd-dev --platform linux/amd64 .",
    "container-dev": "docker run -p 8080:8080 fmd-dev",
    "create-service": "kubectl apply -f ./src/config/kube/service.kube.yaml",
    "create-deployment": "kubectl apply -f ./src/config/kube/deployment.kube.yaml",
    "get-service": "minikube service eatsservice",
    "mocha": "mocha test/mocha",
    "chai": "mocha test/chai",
    "super":"mocha test/supertest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "artillery-plugin-metrics-by-endpoint": "^1.2.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.2",
    "commander": "^11.1.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-compression": "^1.0.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.2.0",
    "mongoose": "^7.6.3",
    "mongoose-paginate-v2": "^1.7.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "update": "^0.7.4",
    "winston": "^3.11.0",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "chai": "^5.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
